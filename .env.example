# Azure Redis Configuration
# Copy this file to .env and fill in your values

# Azure Redis instance details
AZURE_REDIS_HOST=your-redis-instance.redis.cache.windows.net
AZURE_REDIS_PORT=6380
AZURE_REDIS_USERNAME=default

# Azure Entra ID access token (get this using Azure CLI)
# az account get-access-token --resource https://redis.azure.com --query accessToken -o tsv
AZURE_REDIS_ACCESS_TOKEN=

# Fallback for local development
REDIS_URL=redis://localhost:6379

# Rails environment
RAILS_ENV=development